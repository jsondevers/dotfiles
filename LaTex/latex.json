{
  "Inline Math $...$": {
    "prefix": "ff",
    "body": ["$$1$$0"],
    "description": "Inline Math with Dollar Delimiters"
  },
  "fraction": {
    "prefix": "f",
    "body": ["\\frac{$1}{$2} $0"]
  },
  "bold": {
    "prefix": "bold",
    "body": ["\\textbf{${1:${TM_SELECTED_TEXT}}} $0"]
  },
  "italic": {
    "prefix": "italic",
    "body": ["\\textit{${1:${TM_SELECTED_TEXT}}} $0"]
  },

  "align*": {
    "prefix": "al",
    "body": ["\\begin{align*}", "\t$0", "\\end{align*}"]
  },
  "lemma": {
    "prefix": "lem",
    "body": ["\\begin{lemma}", "\t$0", "\\end{lemma}"]
  },
  "proof": {
    "prefix": "pf",
    "body": ["\\begin{proof}", "\t$0", "\\end{proof}"]
  },
  "theorem": {
    "prefix": "thm",
    "body": ["\\begin{theorem}", "\t$0", "\\end{theorem}"]
  },
  "corollary": {
    "prefix": "cor",
    "body": ["\\begin{corollary}", "\t$0", "\\end{corollary}"]
  },
  "definition": {
    "prefix": "def",
    "body": ["\\begin{definition}", "\t$0", "\\end{definition}"]
  },
  "remark": {
    "prefix": "rem",
    "body": ["\\begin{remark}", "\t$0", "\\end{remark}"]
  },
  "exponent": {
    "prefix": "ex",
    "body": ["^{${1:2}}$0"]
  },
  "subscript": {
    "prefix": "sub",
    "body": ["_{$1}$0"]
  },
  "display style summation": {
    "prefix": "sum",
    "body": ["\\sum_{${1:i=1}}^{${2:n}}$0"]
  },
  "floor": {
    "prefix": "floor",
    "body": ["\\left\\lfloor {1:1} \\right\\rfloor$0"]
  },
  "ceil": {
    "prefix": "ceil",
    "body": ["\\left\\lceil {1:1} \\right\\rceil$0"]
  },
  "parentheses": {
    "prefix": "paren",
    "body": ["\\left( {1:1} \\right)$0"]
  },
  "underline": {
    "prefix": "ul",
    "body": ["\\underline{${1:1}}$0"]
  },
  "overline": {
    "prefix": "ol",
    "body": ["\\overline{${1:1}}$0"]
  }
}
